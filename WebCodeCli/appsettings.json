{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:5000",
  "Authentication": {
    "Enabled": true,
    "Users": [
      {
        "Username": "admin",
        "Password": "admin@123"
      },
      {
        "Username": "user",
        "Password": "user123"
      }
    ]
  },
  "CliTools": {
    "MaxConcurrentExecutions": 3,
    "DefaultTimeoutSeconds": 300,
    "EnableCommandWhitelist": true,
    "TempWorkspaceRoot": "D:\\Temp\\WebCodeCli\\Workspaces",
    "WorkspaceExpirationHours": 24,
    "NpmGlobalPath": "C:\\Users\\28651\\AppData\\Roaming\\npm\\",
    "Tools": [
      {
        "Id": "claude-code",
        "Name": "Claude Code",
        "Description": "Anthropic Claude AI 代码助手 (print + stream-json)",
        "Command": "claude",
        "ArgumentTemplate": "-p --verbose --output-format=stream-json --dangerously-skip-permissions \"{prompt}\"",
        "WorkingDirectory": "",
        "Enabled": true,
        "TimeoutSeconds": 0,
        "EnvironmentVariables": {}
      },
      {
        "Id": "codex",
        "Name": "Codex",
        "Description": "Cli",
        "Command": "codex.cmd",
        "ArgumentTemplate": "exec --skip-git-repo-check --dangerously-bypass-approvals-and-sandbox  --json {prompt}",
        "WorkingDirectory": "",
        "Enabled": true,
        "TimeoutSeconds": 0,
        "EnvironmentVariables": {
          "PYTHONIOENCODING": "utf-8",
          "PYTHONLEGACYWINDOWSSTDIO": "utf-8"
        }
      },
      {
        "Id": "qwen-code",
        "Name": "QwenCode",
        "Description": "通义千问代码助手",
        "Command": "qwen",
        "ArgumentTemplate": "code {prompt}",
        "WorkingDirectory": "",
        "Enabled": false,
        "TimeoutSeconds": 300,
        "EnvironmentVariables": {}
      },
      {
        "Id": "gemini-cli",
        "Name": "Gemini CLI",
        "Description": "Google Gemini 命令行工具",
        "Command": "gemini",
        "ArgumentTemplate": "{prompt}",
        "WorkingDirectory": "",
        "Enabled": false,
        "TimeoutSeconds": 300,
        "EnvironmentVariables": {}
      },
      {
        "Id": "copilot-cli",
        "Name": "GitHub Copilot CLI",
        "Description": "GitHub Copilot 命令行工具",
        "Command": "gh",
        "ArgumentTemplate": "copilot suggest {prompt}",
        "WorkingDirectory": "",
        "Enabled": false,
        "TimeoutSeconds": 300,
        "EnvironmentVariables": {}
      }
    ]
  },
  "FrontendPreview": {
    "PortRangeStart": 8010,
    "PortRangeEnd": 9000,
    "MaxConcurrentServers": 10,
    "ServerStartupTimeout": 120,
    "EnableHotReload": true
  }
}
